{
  "name": "cpportfolio",
  "private": true,
  "description": "My personal portfolio",
  "version": "0.0.1",
  "scripts": {
    "build": "cleandir ./.cache && gatsby build",
    "develop": "cleandir ./.cache && gatsby develop",
    "develop-m": "gatsby develop -H 0.0.0.0 -p 8000",
    "format": "prettier --write src/**/*.{js,jsx}",
    "lint": "eslint --ext .js src/",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "serve-m": "gatsby serve -H 0.0.0.0 -p 8000",
    "test": "echo \"Write tests!\"",
    "precommit": "NODE_ENV=production lint-staged"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "^2.5.5",
    "gatsby-plugin-flow": "^1.0.5",
    "gatsby-plugin-html-attributes": "^1.0.5",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-source-filesystem": "^2.0.36",
    "gatsby-transformer-remark": "^2.3.12",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-fontawesome": "^1.6.1",
    "react-headroom": "^2.2.8",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.0.8",
    "react-reveal": "^1.2.2",
    "react-scroll-section": "^1.3.0",
    "react-text-loop": "^2.0.2",
    "react-tippy": "^1.2.3",
    "react-visibility-sensor": "^5.0.2",
    "rebass": "^3.1.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "clean-dir": "^1.0.3",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint src/ --fix",
      "git add"
    ]
  }
}
