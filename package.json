{
  "name": "cpportfolio",
  "private": true,
  "description": "My personal portfolio",
  "version": "0.0.1",
  "scripts": {
    "build": "cleandir ./.cache && gatsby build",
    "develop": "cleandir ./.cache && gatsby develop",
    "develop-m": "gatsby develop -H 0.0.0.0 -p 8000",
    "format": "prettier --write src/**/*.{js,jsx}",
    "lint": "eslint --ext .js src/",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "serve-m": "gatsby serve -H 0.0.0.0 -p 8000",
    "test": "echo \"Write tests!\"",
    "precommit": "NODE_ENV=production lint-staged"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "^2.20.12",
    "gatsby-plugin-flow": "^1.2.1",
    "gatsby-plugin-html-attributes": "^1.0.5",
    "gatsby-plugin-styled-components": "^3.2.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^5.25.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-fontawesome": "^1.7.1",
    "react-headroom": "^2.2.8",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.3.1",
    "react-reveal": "^1.2.2",
    "react-scroll-section": "^1.3.0",
    "react-text-loop": "^2.3.0",
    "react-tippy": "^1.3.4",
    "react-visibility-sensor": "^5.1.1",
    "rebass": "^3.2.2",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "clean-dir": "^1.0.3",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint src/ --fix",
      "git add"
    ]
  }
}
